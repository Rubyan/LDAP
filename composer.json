{
    "name": "rubyan/ldap",
    "description": "LDAP plugin for CakePHP 3.x",
    "type": "cakephp-plugin",
    "keywords":[
        "cakephp",
        "authenticate",
        "ldap"
    ],
    "homepage": "http://github.com/Rubyan/LDAP",
    "authors":[
        {
            "name":"rubyan",
            "role":"Author",
            "homepage":"https://github.com/rubyan"
        },		
    ],
    "license": "MIT",
    "support":{
        "source":"https://github.com/Rubyan/LDAP",
        "issues":"https://github.com/Rubyan/LDAP/issues"
    },
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Rubyan\\LDAP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rubyan\\LDAP\\Test\\": "tests"
        }
    }
}
